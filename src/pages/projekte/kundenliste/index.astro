---
import BaseLayout from '../../../layouts/base.astro';
import Seo from '../../../components/content/seo.astro'
import AppConfig from '../../../components/config.js'
import PageTitle from '../../../components/content/page-title/page-title.astro';
import Text from '../../../components/content/global/global-text.astro';
import Lead from '../../../components/content/global/global-lead.astro';
import ProjectList from '../../../components/content/project/project-list.astro';
---
<BaseLayout>
  <Seo slot="seo" pageTitle="Kundenliste">
    <meta
      content="Kundenliste"
      name="description">
    <meta content="Kundenliste,Kunden,Projekte,Referenzen,sotlze Ergebnisse" name="keywords">
    <meta content="summary" name="twitter:card">
    <meta content="Technisches SEO" name="twitter:title">
    <meta
      content="Kundenliste"
      name="twitter:description">
    <meta content={ new URL(Astro.url.pathname, Astro.site) } name="twitter:url">
    <meta
      content={ AppConfig.siteBaseUrl + '/assets/services/websites-e-commerce.jpg' }
      name="twitter:image">
    <meta content="website" property="og:type">
    <meta content="Kundenliste" property="og:title">
    <meta content="Kundenliste" property="og:site_name">
    <meta content="de" property="og:locale">
    <meta
      content="Kundenliste"
      property="og:description">
    <meta content={ new URL(Astro.url.pathname, Astro.site) } property="og:url">
    <meta
      content={ AppConfig.siteBaseUrl + '/assets/services/websites-e-commerce.jpg' }
      property="og:image">
  </Seo>
  <PageTitle title="Unsere Kundenliste" />
  <ProjectList />
</BaseLayout>
