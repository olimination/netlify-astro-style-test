    -moz-transition: all 0.3s ease-in-out;
    -ms-transition: all 0.3s ease-in-out;
    -o-transition: all 0.3s ease-in-out;
    transition: all 0.3s ease-in-out;
  }

  figure:hover:before {
    background-color: rgba(242, 166, 51, 0);
  }

  figure svg,
  body.darkmode .service-technologies.darkmode figure svg {
    position: absolute;
    height: 100%;
    width: 100%;
    top: 0;
    left: 0;
